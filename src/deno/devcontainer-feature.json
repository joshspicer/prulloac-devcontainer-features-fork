{
    "name": "Deno",
    "id": "deno",
    "version": "1.0.0",
    "description": "Installs Deno (Javascript runtime written in rust)",
    "documentationURL": "https://github.com/prulloac/devcontainer-features/tree/main/src/deno",
    "options": {
        "version": {
            "type": "string",
            "default": "latest",
            "description": "Version of Deno to install"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "dbaeumer.vscode-eslint"
            ]
        }
    },
    "containerEnv": {
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
